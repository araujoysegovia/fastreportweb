    private void Text26_BeforePrint(object sender, EventArgs e)
    {
      String Query_INDCPITRA = ToString(Report.GetColumnValue(&quot;Query.INDCPITRA&quot;));
      String Query_IDTIPDOC = ToString(Report.GetColumnValue(&quot;Query.IDTIPDOC&quot;));
      String Query_TIPOFAC = ToString(Report.GetColumnValue(&quot;Query.TIPOFAC&quot;));
      String Query_DESCRITRA = ToString(Report.GetColumnValue(&quot;Query.DESCRITRA&quot;));
      String Query_NUMEFAC = ToString(Report.GetColumnValue(&quot;Query.NUMEFAC&quot;));
      String Query_VENCEFAC = ToString(Report.GetColumnValue(&quot;Query.VENCEFAC&quot;));
      String Query_REFEFAC = ToString(Report.GetColumnValue(&quot;Query.REFEFAC&quot;));
      String Query_IDBANCO = ToString(Report.GetColumnValue(&quot;Query.IDBANCO&quot;));
      String Query_IDPLAZA = ToString(Report.GetColumnValue(&quot;Query.IDPLAZA&quot;));
      Decimal Query_PORRETETRA = ToDecimal(Report.GetColumnValue(&quot;Query.PORRETETRA&quot;));
      Decimal Query_BASERETETRA = ToDecimal(Report.GetColumnValue(&quot;Query.BASERETETRA&quot;));
      
      /**** Mjunco Req. 29214 2016-02-16 ***/
      String Query_MuestraDescriTra = ToString(Report.GetColumnValue(&quot;Query.MuestraDescrCta&quot;));
       /*
         No mantener
         En Cuentas de Caja - CxC y CxP
         Solo en Cuentas de Caja
         Solo en CxC
         Solo en CxP
         En Cuentas de Caja y CxC
         En Cuentas de Caja y CxP
         En CxC y CxP   
        */
      switch (Query_MuestraDescriTra){
       case &quot;No mantener&quot;:
        CadDescriTraCXC=Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC;   
        CadDescriTraCXP=Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC;   
        CadDescriTraCAJBCO=Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC + &quot; &quot; + Query_IDBANCO + &quot; &quot; + Query_IDPLAZA;      
        break;
       case &quot;En Cuentas de Caja - CxC y CxP&quot;:
        CadDescriTraCXC=Query_DESCRITRA;
        CadDescriTraCXP=Query_DESCRITRA;
        CadDescriTraCAJBCO=Query_DESCRITRA;
        break;
       case  &quot;Solo en Cuentas de Caja&quot;:
        CadDescriTraCXC=Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC; 
        CadDescriTraCXP=Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC;
        CadDescriTraCAJBCO=Query_DESCRITRA;
        break;
       case &quot;Solo en CxC&quot;:
        CadDescriTraCXC=Query_DESCRITRA;
        CadDescriTraCXP=Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC;
        CadDescriTraCAJBCO=Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC + &quot; &quot; + Query_IDBANCO + &quot; &quot; + Query_IDPLAZA;      
        break;
       case &quot;Solo en CxP&quot;:
        CadDescriTraCXC=Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC;
        CadDescriTraCXP=Query_DESCRITRA;
        CadDescriTraCAJBCO=Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC + &quot; &quot; + Query_IDBANCO + &quot; &quot; + Query_IDPLAZA;      
        break;
       case &quot;En Cuentas de Caja y CxC&quot;:
        CadDescriTraCXC=Query_DESCRITRA;
        CadDescriTraCXP=Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC;
        CadDescriTraCAJBCO=Query_DESCRITRA;
        break;
       case &quot;En Cuentas de Caja y CxP&quot;:
        CadDescriTraCXC=Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC;
        CadDescriTraCXP=Query_DESCRITRA;
        CadDescriTraCAJBCO=Query_DESCRITRA;
        break;
       case &quot;En CxC y CxP&quot;:
        CadDescriTraCXC=Query_DESCRITRA;
        CadDescriTraCXP=Query_DESCRITRA;
        CadDescriTraCAJBCO=Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC + &quot; &quot; + Query_IDBANCO + &quot; &quot; + Query_IDPLAZA;      
        break;
      }
      
      /*************************************/
      
     if( Query_INDCPITRA == &quot;2&quot; || Query_INDCPITRA == &quot;3&quot; || Query_INDCPITRA == &quot;6&quot; ){
      if( Query_IDTIPDOC == &quot;003&quot; &amp;&amp; Query_TIPOFAC == &quot;EFE&quot; ){
       Text26.Text = Query_DESCRITRA;
      }else{
       if(  Query_INDCPITRA == &quot;2&quot;){
        Text26.Text = CadDescriTraCXC;//Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC;
       }else{
        if(  Query_INDCPITRA == &quot;3&quot; ){
         Text26.Text = CadDescriTraCXP;//Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC;
        }else{
         Text26.Text = CadDescriTraCAJBCO;//Query_TIPOFAC + &quot; &quot; + Query_NUMEFAC + &quot; &quot; + Query_VENCEFAC + &quot; &quot; + Query_REFEFAC + &quot; &quot; + Query_IDBANCO + &quot; &quot; + Query_IDPLAZA;
        }
       }
      }
      }else{
        if( Query_PORRETETRA != 0 ){
          Text26.Text = &quot;IMP. &quot; + ToString(FormatNumber(Query_PORRETETRA)) + &quot;% Base. &quot; + ToString(FormatNumber(Query_BASERETETRA));
        }else{
          Text26.Text = Query_DESCRITRA;
        }
      }
    }

