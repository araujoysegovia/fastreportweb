    private void GroupHeader3_BeforePrint(object sender, EventArgs e)
    {
      string Documento = ToString(Report.GetColumnValue(&quot;Query.Docum_Fhe&quot;));  
      Termino = false;
      Child2.Visible = true;
      if(Engine.FirstPass)
      {                   
        if(!Documento.Equals(DocumentoActual))
        {          
          DocumentoActual = ToString(Report.GetColumnValue(&quot;Query.Docum_Fhe&quot;));            
          TotalPages.Insert(contDocumento, PaginaDocumento - 1);                    
          contDocumento ++;
          PaginaDocumento = 1;          
        } 
        else
        {
          //MessageBox.Show(&quot;Tpag: &quot; + ToString(PaginaTotal) + &quot; Dpag: &quot; + ToString(PaginaDocumento) + &quot;DocA: &quot; + DocumentoActual + &quot;Doc: &quot; + Documento);
        }
      }
      if(Engine.FinalPass)
      {  
        if(PaginaTotal == 1)
        {
          DocumentoActual = ToString(Report.GetColumnValue(&quot;Query.DOCUM_FHE&quot;));
        }
        if(!Documento.Equals(DocumentoActual))
        {    
          concep = 0;
          DocumentoActual = ToString(Report.GetColumnValue(&quot;Query.Docum_Fhe&quot;));
          contDocumento ++;     
          Memo13.Text = &quot;1 de &quot; + ToString(TotalPages[contDocumento]);                      
          PaginaDocumento = 1;    
          
        }
        else
        {
          Memo13.Text = PaginaDocumento.ToString() + &quot; de &quot; + ToString(TotalPages[contDocumento]); 
        }        
      }
      PaginaTotal ++;
      PaginaDocumento ++;
      
      contSubTotales = 1;        
      DataSourceBase SubTotales; 
      SubTotales = Report.GetDataSource(&quot;Query3&quot;);
      SubTotales.Init();
      while (SubTotales.HasMoreRows)
      { 
        contSubTotales ++;
        SubTotales.Next();
      }
      if (contSubTotales &gt;= 7)
      {
        double alto = 3.78 * 5;
        alto = alto * contSubTotales;
        BOX4.Height = Convert.ToSingle(alto);
        BOX5.Height = Convert.ToSingle(alto);
        Text39.Height = BOX5.Height;
        //Cuotas.Top = Convert.ToSingle(&quot;181.44&quot;);
        Child6.Height = Convert.ToSingle(alto) + Convert.ToSingle(&quot;124.74&quot;);
        Cuotas.Top = Convert.ToSingle(alto) + Convert.ToSingle(&quot;41.58&quot;);
      }
    }

