    private void GroupFooter2_BeforePrint(object sender, EventArgs e)
    {
      if(((Int32)Report.GetColumnValue(&quot;Query.TotalConcep&quot;))==concep)
      {
        if((Int32)TotalPages[contDocumento]&gt;1)
        {                
          if((Int32)Pages[contDocumento] == PaginaDocumento - 1)
          {
            Child6.PrintOn = Child1.PrintOn;
            Child2.Visible = false;
          }
          else
          {
            Child6.PrintOn = GroupFooter1.PrintOn;
            Child2.Visible = true;
            GroupFooter2.Visible = false;
          }
        }
        else
        {         
          Child6.PrintOn = GroupFooter1.PrintOn;
        }
      }
      else
      {
        Child2.Visible = false;
        Child5.Visible = false;
      }
    }

    private void Child5_BeforePrint(object sender, EventArgs e)
    {
      if(((Int32)Report.GetColumnValue(&quot;Query.TotalConcep&quot;))==concep &amp;&amp; (Int32)Pages[contDocumento] == PaginaDocumento - 1  )
      {
        Child5.Visible = true;
        GroupFooter2.Visible = true;
      }
      else
      {
        Child5.Visible = false;
        if(((Int32)Report.GetColumnValue(&quot;Query.TotalConcep&quot;))==concep)
        {
          GroupFooter2.Visible = false;
        }
        else
        {
          GroupFooter2.Visible = true;
        }
      }
    }

