    private void GroupHeader3_BeforePrint(object sender, EventArgs e)
    {
      string Documento = ToString(Report.GetColumnValue(&quot;Query.Docum_Fhe&quot;));  
      Termino = false;
      Child2.Visible = true;
      
      Text46.Text = Report.GetColumnValue(&quot;Query.NumeroFacturaEspecial&quot;).ToString();
      Text47.Text = Report.GetColumnValue(&quot;Query.NORESOLUCION&quot;).ToString();
      NumeroFactura = Report.GetColumnValue(&quot;Query.numefac_fhe&quot;).ToString();
      
      if(Engine.FirstPass)
      {                   
        if(!Documento.Equals(DocumentoActual))
        {          
          DocumentoActual = ToString(Report.GetColumnValue(&quot;Query.Docum_Fhe&quot;));            
          TotalPages.Insert(contDocumento, PaginaDocumento - 1);                    
